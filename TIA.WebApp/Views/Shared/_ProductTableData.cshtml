@model ProductDTO

<tr id="product-@Model.Id">
    <td>@Model.Title</td>
    <td>@Model.Description</td>
    <td>@Model.Price</td>
    <td>
        <a class="btn btn-primary popupCreateEdit" data-url="/Product/CreateEdit" data-item-id="@Model.Id" data-parent-id="@Model.ParentCatalogId"
           data-toggle="modal" data-target="#modal-view" id="Product-E">
            /
        </a>
    </td>
    <td>
        <a class="btn btn-primary popupDelete" data-url="/Product/Delete" data-id="@Model.Id"
           data-toggle="modal" data-target="#modal-view" id="Product-D">
            -
        </a>
    </td>
</tr>